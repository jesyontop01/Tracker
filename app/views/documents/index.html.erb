<p id="notice"><%= notice %></p>

<h1>Documents</h1>

<table>
  <thead>
    <tr>
      <th>Series</th>
      <th>Year</th>
      <th>Registrationno</th>
      <th>Candidaterange</th>
      <th>Request</th>
      <th>Attachment</th>
      <th>Email</th>
      <th>Created Date:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documents.each do |document| %>
      <tr>
        <td><%= document.series %></td>
        <td><%= document.year %></td>
        <td><%= document.registrationNo %></td>
        <td><%= document.CandidateRange %></td>
        <td><%= document.request %></td>
        <td><%= document.attachment %></td>
        <td><%= document.email %></td>
        <td><%= document.created_at %></td>
        <td><%= link_to 'Show', document %></td>
        <td><%= link_to 'Edit', edit_document_path(document) %></td>
        <td><%= link_to 'Destroy', document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document', new_document_path %>
